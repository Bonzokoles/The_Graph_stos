# ========== DATABASES ==========
# SQLite
DATABASE_URL=sqlite:///./jimbo.db

# ChromaDB
CHROMA_HOST=localhost
CHROMA_PORT=8001
CHROMA_PERSIST_DIRECTORY=./chromadb_data

# PostgreSQL (optional)
POSTGRES_URL=postgresql://user:password@localhost:5432/jimbo

# MongoDB (optional)
MONGODB_URL=mongodb://localhost:27017/jimbo

# Redis (optional)
REDIS_URL=redis://localhost:6379

# ========== VECTOR STORES ==========
# Pinecone
PINECONE_API_KEY=your-pinecone-key-here
PINECONE_ENVIRONMENT=us-east-1-aws

# Weaviate
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=your-weaviate-key-here

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-key-here

# ========== INTEGRATIONS ==========
# Slack
SLACK_BOT_TOKEN=xoxb-your-slack-token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Discord
DISCORD_BOT_TOKEN=your-discord-bot-token
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/WEBHOOK/URL

# Telegram
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Twitter/X
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
TWITTER_ACCESS_TOKEN=your-twitter-access-token
TWITTER_ACCESS_SECRET=your-twitter-access-secret

# Email (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-email-password

# Twilio (SMS)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# GitHub
GITHUB_TOKEN=ghp-your-github-token

# Google Drive
GOOGLE_DRIVE_CLIENT_ID=your-google-client-id
GOOGLE_DRIVE_CLIENT_SECRET=your-google-client-secret

# Notion
NOTION_API_KEY=secret_your-notion-key

# Airtable
AIRTABLE_API_KEY=your-airtable-key

# AWS S3
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name

# ========== APPLICATION ==========
# App settings
NODE_ENV=development
PORT=5173
API_PORT=8000

# Security
JWT_SECRET=your-jwt-secret-key-change-this
ENCRYPTION_KEY=your-encryption-key-32-chars-min

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# ========== FEATURES ==========
# Enable/disable features
ENABLE_AI_FEATURES=true
ENABLE_VECTOR_STORES=true
ENABLE_CLOUD_STORAGE=false
ENABLE_ANALYTICS=false
ENABLE_TELEMETRY=false